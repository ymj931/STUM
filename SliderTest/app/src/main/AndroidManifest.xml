<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.administrator.slidertest" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>

        <activity
            android:name=".NavDrawerItem"
            android:label="@string/title_activity_nav_drawer_item" >
        </activity>
        <activity
            android:name=".NavDrawerListAdapter"
            android:label="@string/title_activity_nav_drawer_list_adapter" >
        </activity>
        <activity
            android:name=".BaseActivity"
            android:label="@string/title_activity_base" >
        </activity>
        <activity
            android:name=".First"
            android:label="@string/title_activity_first" >
        </activity>
        <activity
            android:name=".Two"
            android:label="@string/title_activity_two" >
        </activity>
    </application>

</manifest>
